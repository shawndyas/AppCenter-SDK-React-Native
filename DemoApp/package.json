{
  "name": "Outpatient",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start-web": "./node_modules/.bin/webpack-dev-server -d --config ./web/webpack.config.js --inline --hot --colors",
    "build-web": "./node_modules/.bin/webpack -p --config ./web/webpack.config.js",
    "upload-sourcemaps": "npm run upload-sourcemaps-ios; npm run upload-sourcemaps-android",
    "upload-sourcemaps-ios": "(GIT_COMMIT=`git rev-parse HEAD`; curl https://api.rollbar.com/api/1/sourcemap -F access_token=0dbf70f633c645929104a439cb9e6c4f -F version=$GIT_COMMIT.ios -F minified_url=http://reactnativehost/main.jsbundle -F source_map=@sourcemap.ios.js.map -F index.js=@index.js) || true",
    "upload-sourcemaps-android": "(GIT_COMMIT=`git rev-parse HEAD`; curl https://api.rollbar.com/api/1/sourcemap -F access_token=0dbf70f633c645929104a439cb9e6c4f -F version=$GIT_COMMIT.android -F minified_url=http://reactnativehost/index.android.bundle -F source_map=@sourcemap.android.js.map -F index.js=@index.js) || true",
    "test": "detox build && detox test --cleanup",
    "test-jest": "jest"
  },
  "dependencies": {
    "appcenter": "1.9.0",
    "appcenter-crashes": "1.9.0",
    "axios": "^0.18.0",
    "date-fns": "^1.29.0",
    "firebase": "5.5.7",
    "history": "4.7.2",
    "i18n-js": "3.1.0",
    "instabug-reactnative": "8.0.18",
    "lodash": "4.17.11",
    "md5": "2.2.1",
    "moment-timezone": "0.5.23",
    "normalizr": "^3.2.4",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-native": "github:Outpatient/react-native#0.56.0_uiviewmanager_patch",
    "react-native-actionsheet": "2.4.2",
    "react-native-branch": "2.3.3",
    "react-native-calendar-events": "github:Outpatient/react-native-calendar-events#28b287004c00fae21f2070070e45579bed9bf282",
    "react-native-calendars": "1.21.0",
    "react-native-camera": "1.3.1",
    "react-native-camera-roll-picker": "1.2.3",
    "react-native-code-push": "5.4.2",
    "react-native-contacts": "2.2.4",
    "react-native-device-info": "0.24.3",
    "react-native-dialog": "^5.1.0",
    "react-native-fbsdk": "0.8.0",
    "react-native-firebase": "5.0.0",
    "react-native-fs": "2.11.18",
    "react-native-gifted-chat": "0.4.3",
    "react-native-google-places-autocomplete": "1.3.9",
    "react-native-heic-converter": "1.1.1",
    "react-native-hyperlink": "0.0.14",
    "react-native-idfa": "4.1.0",
    "react-native-keychain": "3.0.0",
    "react-native-mail": "3.0.7",
    "react-native-map-link": "^2.1.0",
    "react-native-maps": "0.22.0",
    "react-native-navigation": "1.1.483",
    "react-native-permissions": "1.1.1",
    "react-native-piwik": "github:Outpatient/react-native-matomo#1ceef6d882cf3213eb51cce54c4e6734a1cfe1f1",
    "react-native-section-list-get-item-layout": "2.2.3",
    "react-native-slowlog": "1.0.2",
    "react-native-swipe-gestures": "1.0.2",
    "react-native-swipeable-row": "0.8.1",
    "react-native-swiper": "1.5.13",
    "react-native-switch": "^1.5.0",
    "react-native-vector-icons": "6.1.0",
    "react-native-version-number": "0.3.5",
    "react-native-web": "0.9.6",
    "react-redux": "5.1.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-router-native": "4.3.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "recompose": "^0.30.0",
    "redux": "4.0.1",
    "redux-persist": "5.10.0",
    "redux-thunk": "2.3.0",
    "reselect": "^4.0.0",
    "rn-fetch-blob": "https://github.com/LorienHW/rn-fetch-blob/tarball/master",
    "rollbar-react-native": "0.3.3",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/preset-env": "^7.2.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-native-web": "0.9.6",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-preset-react-native": "5.0.2",
    "detox": "9.0.6",
    "eslint-plugin-react": "7.11.1",
    "husky": "1.1.3",
    "jest": "^23.6.0",
    "mocha": "5.2.0",
    "prettier": "1.14.3",
    "pretty-quick": "1.8.0",
    "react-test-renderer": "16.6.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-logger": "^3.0.6",
    "url-loader": "1.1.2",
    "webpack": "4.24.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "jest": {
    "preset": "react-native",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [],
    "testPathIgnorePatterns": [
      "/e2e/"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Outpatient.app",
        "build": "xcodebuild -workspace ios/Outpatient.xcworkspace -scheme Outpatient -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm test"
    }
  }
}
