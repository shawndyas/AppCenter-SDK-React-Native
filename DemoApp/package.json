    "lint": "./node_modules/.bin/eslint ."
  },
  "dependencies": {
    "appcenter": "^1.12.0",
    "appcenter-analytics": "^1.12.0",
    "appcenter-crashes": "^1.12.0",
    "appcenter-push": "^1.12.0",
    "metro": "0.48.1",
    "react": "16.6.3",
    "react-native": "github:Outpatient/react-native#0.56.0_uiviewmanager_patch",
    "react-native-fs": "^2.9.12",
    "react-native-image-picker": "^0.28.0",
    "react-native-modal-selector": "^1.0.2",
    "react-native-simple-toast": "0.0.8",
    "react-navigation": "^2.11.2"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/plugin-external-helpers": "7.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "23.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "jest": "22.4.3",
    "metro-react-native-babel-preset": "^0.45.3",
    "prop-types": "15.6.0",
    "react-test-renderer": "16.6.3"
